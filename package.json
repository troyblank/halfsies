{
    "name": "Halfsies",
    "version": "1.0.0",
    "scripts": {
        "start": "next -p 8888",
        "preview": "npm run build && cd out && serve -p 8888",
        "build": "grunt build && next build && next export",
        "clean": "rm -rf .next && rm -rf out",
        "test": "npm run unit && npm run lint",
        "unit": "nyc --reporter=text-summary --reporter=lcov --reporter=cobertura mocha \"./src/**/*.spec.js\"",
        "lint": "npm run eslint && npm run sasslint",
        "eslint": "eslint './{pages,src}/**/*.js'",
        "eslintFix": "eslint --fix './{pages,src}/**/*.js'",
        "sasslint": "./node_modules/sass-lint/bin/sass-lint.js -c ./node_modules/@troyblank/sasslint-config-troyblank/.sass-lint.yml './src/**/*.scss' -v -q"
    },
    "nyc": {
        "temp-directory": "./coverage/.nyc_output"
    },
    "dependencies": {
        "@babel/plugin-proposal-export-default-from": "7.5.2",
        "@zeit/next-sass": "1.0.1",
        "amazon-cognito-identity-js": "3.1.2",
        "classnames": "2.2.6",
        "grunt": "0.4.5",
        "grunt-cli": "1.3.2",
        "grunt-grunticon": "2.3.2",
        "load-grunt-config": "0.17.1",
        "next": "9.1.3",
        "node-sass": "4.12.0",
        "normalize.css": "8.0.1",
        "react": "16.10.2",
        "react-dom": "16.10.1",
        "react-redux": "7.1.1",
        "redux": "4.0.4",
        "redux-thunk": "2.3.0"
    },
    "devDependencies": {
        "@babel/register": "7.6.2",
        "@testing-library/react": "9.3.0",
        "@troyblank/eslint-config-troyblank": "1.1.0",
        "@troyblank/mocha-base-troyblank": "1.0.0",
        "@troyblank/sasslint-config-troyblank": "1.0.0",
        "eslint": "6.6.0",
        "mocha": "2.2.4",
        "nyc": "14.1.1",
        "serve": "11.2.0"
    },
    "author": "Troy Blank",
    "license": "BSD-3-Clause"
}
