{
    "name": "Halfsies",
    "version": "1.0.0",
    "scripts": {
        "start": "next -p 8888",
        "preview": "npm run build && cd out && serve -p 8888",
        "build": "grunt build && next build && next export && npm run copy",
        "copy": "cp _redirects out/_redirects",
        "clean": "rm -rf .next && rm -rf out",
        "test": "npm run lint && npm run unit",
        "test-once": "npm run lint && npm run unit-once",
        "unit": "jest --watch --coverage",
        "unit-once": "jest --coverage",
        "lint": "tsc && npm run eslint && npm run sasslint",
        "eslint": "eslint './{pages,src}/**/*.{js,ts,tsx}'",
        "eslintFix": "eslint --fix './{pages,src}/**/*.{js,ts,tsx}'",
        "sasslint": "./node_modules/sass-lint/bin/sass-lint.js -c ./node_modules/@troyblank/sasslint-config-troyblank/.sass-lint.yml './src/**/*.scss' -v -q"
    },
    "dependencies": {
        "@babel/plugin-proposal-export-default-from": "7.8.3",
        "@types/jest": "25.2.1",
        "@types/react": "16.9.12",
        "amazon-cognito-identity-js": "4.2.1",
        "classnames": "2.2.6",
        "grunt": "0.4.5",
        "grunt-cli": "1.3.2",
        "grunt-grunticon": "2.3.2",
        "load-grunt-config": "0.17.1",
        "next": "9.3.5",
        "node-sass": "4.13.1",
        "normalize.css": "8.0.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-redux": "7.2.0",
        "redux": "4.0.5",
        "redux-thunk": "2.3.0",
        "typescript": "3.8.3"
    },
    "devDependencies": {
        "@babel/register": "7.6.2",
        "@testing-library/react": "10.0.2",
        "@troyblank/eslint-config-troyblank": "1.4.1",
        "@troyblank/mocha-base-troyblank": "1.0.0",
        "@troyblank/jest-base": "0.1.0",
        "@troyblank/sasslint-config-troyblank": "1.0.0",
        "coveralls": "3.0.11",
        "eslint": "6.6.0",
        "serve": "11.3.0"
    },
    "author": "Troy Blank",
    "license": "BSD-3-Clause"
}
