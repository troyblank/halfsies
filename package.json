{
    "name": "Halfsies",
    "version": "1.0.0",
    "main": "server.js",
    "scripts": {
        "start": "next -p 8888",
        "startOld": "grunt build && node server",
        "preview": "npm run build && cd out && serve -p 8888",
        "build": "grunt build && next build && next export",
        "clean": "rm -rf .next && rm -rf out",
        "testOld": "grunt test",
        "test": "npm run unit && npm run lint",
        "unit": "nyc --reporter=text-summary --reporter=lcov --reporter=cobertura mocha \"./src/**/*.spec.js\"",
        "lint": "npm run eslint && npm run sasslint",
        "eslint": "eslint './{pages,src}/**/*.js'",
        "eslintFix": "eslint --fix './{pages,src}/**/*.js'",
        "sasslint": "sass-lint -c ./node_modules/@troyblank/sasslint-config-troyblank/.sass-lint.yml './src/**/*.scss' -v -q"
    },
    "nyc": {
        "temp-directory": "./coverage/.nyc_output"
    },
    "dependencies": {
        "@babel/plugin-proposal-export-default-from": "7.5.2",
        "@zeit/next-sass": "1.0.1",
        "amazon-cognito-identity-js": "3.1.2",
        "aws-sdk": "2.556.0",
        "next": "9.1.1",
        "node-sass": "4.12.0",
        "normalize.css": "8.0.1",
        "react": "16.10.2",
        "react-dom": "16.10.1",
        "react-redux": "7.1.1",
        "redux": "4.0.4",
        "redux-thunk": "2.3.0",
        "colors": "1.1.2",
        "compression": "1.5.0",
        "connect-flash": "0.1.1",
        "connect-redis": "2.4.0",
        "body-parser": "1.12.4",
        "express": "4.12.3",
        "express-handlebars": "2.0.1",
        "express-session": "1.11.3",
        "mongoose": "4.0.6",
        "morgan": "1.9.1",
        "passport": "0.2.2",
        "passport-local": "1.0.0",
        "redis": "0.12.1"
    },
    "devDependencies": {
        "@babel/register": "7.6.2",
        "@testing-library/react": "9.3.0",
        "@troyblank/eslint-config-troyblank": "1.1.0",
        "@troyblank/mocha-base-troyblank": "1.0.0",
        "@troyblank/sasslint-config-troyblank": "1.0.0",
        "chai": "4.1.2",
        "grunt": "0.4.5",
        "grunt-angular-templates": "0.5.7",
        "grunt-bower-task": "0.4.0",
        "grunt-contrib-clean": "0.6.0",
        "grunt-contrib-uglify": "0.9.1",
        "grunt-contrib-watch": "0.6.1",
        "grunt-grunticon": "2.3.2",
        "grunt-mocha-test": "0.12.7",
        "grunt-jslint": "1.1.14",
        "grunt-karma": "0.10.1",
        "grunt-sass": "1.0.0",
        "karma": "0.12.31",
        "karma-chai": "0.1.0",
        "karma-mocha": "0.1.10",
        "karma-mocha-reporter": "1.0.2",
        "grunt-notify": "0.4.1",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-referee": "0.4.2",
        "load-grunt-config": "0.17.1",
        "mocha": "2.2.4",
        "referee": "1.1.1",
        "referee-sinon": "1.0.2",
