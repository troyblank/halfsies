{
	"name": "Halfsies",
	"version": "1.2.0",
	"packageManager": "yarn@1.22.21",
	"scripts": {
		"start": "npm run clean && npm run watch",
		"clean": "rm -rf dist && rm -rf coverage",
		"watch": "next dev",
		"build": "next build",
		"test": "npm run lint && npm run unit:once",
		"unit": "jest --watch",
		"unit:once": "jest --maxWorkers=25%",
		"lint": "tsc --noEmit && eslint src --ext .ts,.tsx && npm run sasslint",
		"sasslint": "./node_modules/sass-lint/bin/sass-lint.js -c ./node_modules/@troyblank/sasslint-config-troyblank/.sass-lint.yml './src/**/*.scss' -v -q"
	},
	"dependencies": {
		"@aws-amplify/adapter-nextjs": "1.0.27",
		"@aws-amplify/auth": "6.0.27",
		"@tanstack/react-query": "5.40.1",
		"@troyblank/eslint-config-troyblank": "2.4.0",
		"@troyblank/sasslint-config-troyblank": "1.0.0",
		"@types/chance": "1.1.6",
		"@types/jest": "29.5.12",
		"@types/node": "20.12.11",
		"@types/react": "18.3.1",
		"aws-amplify": "6.0.27",
		"chance": "1.1.11",
		"classnames": "2.5.1",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"js-cookie": "2.2.1",
		"lodash": "4.17.21",
		"next": "14.1.0",
		"normalize.css": "8.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sass": "1.71.0",
		"typescript": "5.4.5"
	},
	"devDependencies": {
		"@swc/core": "1.5.24",
		"@swc/jest": "0.2.36",
		"@testing-library/dom": "10.1.0",
		"@testing-library/jest-dom": "6.4.5",
		"@testing-library/react": "16.0.0",
		"@testing-library/user-event": "14.5.2"
	},
	"author": "Troy Blank",
	"license": "BSD-3-Clause"
}
