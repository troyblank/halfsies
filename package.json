{
    "name": "Halfsies",
    "version": "1.0.0",
    "scripts": {
        "start": "next -p 8888",
        "preview": "npm run build && cd out && serve -p 8888",
        "build": "next build && next export && npm run copy",
        "copy": "cp _redirects out/_redirects",
        "clean": "rm -rf .next && rm -rf out",
        "test": "npm run lint && npm run unit",
        "test-once": "npm run lint && npm run unit-once",
        "unit": "jest --watch --coverage",
        "unit-once": "jest --coverage",
        "lint": "tsc && npm run eslint && npm run sasslint",
        "eslint": "eslint './{pages,src}/**/*.{js,ts,tsx}'",
        "eslintFix": "eslint --fix './{pages,src}/**/*.{js,ts,tsx}'",
        "sasslint": "./node_modules/sass-lint/bin/sass-lint.js -c ./node_modules/@troyblank/sasslint-config-troyblank/.sass-lint.yml './src/**/*.scss' -v -q"
    },
    "dependencies": {
        "@aws-amplify/auth": "5.6.5",
        "@babel/plugin-proposal-export-default-from": "7.22.17",
        "@types/babel__traverse": "7.17.1",
        "@types/node": "14.0.26",
        "@types/react": "16.9.12",
        "amazon-cognito-identity-js": "4.2.1",
        "aws-amplify": "5.3.11",
        "classnames": "2.2.6",
        "js-cookie": "2.2.1",
        "lodash": "4.17.19",
        "next": "13.5.1",
        "normalize.css": "8.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-redux": "8.1.3",
        "redux": "4.0.5",
        "redux-thunk": "2.3.0",
        "sass": "1.69.5",
        "typescript": "5.2.2"
    },
    "devDependencies": {
        "@babel/register": "7.22.15",
        "@testing-library/react": "10.0.2",
        "@troyblank/eslint-config-troyblank": "2.4.0",
        "@troyblank/mocha-base-troyblank": "1.0.0",
        "@troyblank/sasslint-config-troyblank": "1.0.0",
        "@types/jest": "29.5.8",
        "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
        "chance": "1.1.4",
        "coveralls": "3.0.14",
        "enzyme": "3.11.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0"
    },
    "author": "Troy Blank",
    "license": "BSD-3-Clause"
}
